{
  "name": "integrated",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode dev --open",
    "test": "vite --mode test --open --port 4000",
    "uat": "vite --mode uat --open",
    "prod": "vite --mode prod --open",
    "build:dev": "vite build --mode dev",
    "build:test": "vite build --mode test",
    "build:uat": "vite build --mode uat",
    "build:prod": "vite build --mode prod",
    "preview": "vite preview",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "vue-tsc --noEmit && eslint src --ext .vue,.ts,.js --fix --cache",
    "stylelint": "stylelint 'src/**/*.{vue,css,less}' --cache --fix",
    "format": "npx prettier --write **/*.{md,json,html}",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{md,json,html}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{vue,css,less}": [
      "npm run stylelint"
    ],
    "**/*.{vue,ts,js}": [
      "npm run eslint"
    ]
  },
  "dependencies": {
    "ant-design-vue": "2.2.8",
    "axios": "^0.26.1",
    "echarts": "^5.4.0",
    "echarts-liquidfill": "^3.1.0",
    "js-md5": "^0.7.3",
    "mathjs": "^11.4.0",
    "qs": "^6.11.0",
    "videojs": "^1.0.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "^3.2.25",
    "vue-seamless-scroll": "^1.1.23",
    "vue3-seamless-scroll": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.7",
    "path": "^0.12.7",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "stylelint": "^14.6.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.0",
    "vite-plugin-compression": "^0.5.1",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.29.8",
    "vuex": "^4.0.2"
  }
}
